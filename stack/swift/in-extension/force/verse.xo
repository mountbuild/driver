force get-intents-version-number
  field build, field in-extension

  cause get-aspect
    drive object, share build
    drive aspect, |IntentsVersionNumber|
    raise build, field double

force set-intents-version-number
  field build, field in-extension
  field value, field double

  cause set-aspect
    drive object, share build
    drive aspect, |IntentsVersionNumber|
    drive factor, share value
