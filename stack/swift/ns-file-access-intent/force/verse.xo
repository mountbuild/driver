force get-url
  field build, field ns-file-access-intent

  cause get-aspect
    drive object, share build
    drive aspect, |url|
    raise build, field url

force set-url
  field build, field ns-file-access-intent
  field value, field url

  cause set-aspect
    drive object, share build
    drive aspect, |url|
    drive factor, share value
