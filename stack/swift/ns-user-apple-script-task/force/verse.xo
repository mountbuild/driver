force execute
  field build, field ns-user-apple-script-task
  field with-apple-event, field ns-apple-event-descriptor
  field completion-handler, field ns-user-apple-script-task-completion-handler

  cause call-method
    drive object, share build
    drive method, |execute|
    drive factor
      brand with-apple-event
      share with-apple-event
    drive factor
      brand completion-handler
      share completion-handler
