force upload-task
  field build, field url-session-upload-task
  field with, field url-request
  field from, field data

  cause call-method
    drive object, share build
    drive method, |uploadTask|
    drive factor
      brand with
      share with
    drive factor
      brand from
      share from
    raise build, class url-session-upload-task

