force get-domain
  field build, field http-cookie

  cause get-aspect
    drive object, share build
    drive aspect, |domain|
    raise build, field string

force set-domain
  field build, field http-cookie
  field value, field string

  cause set-aspect
    drive object, share build
    drive aspect, |domain|
