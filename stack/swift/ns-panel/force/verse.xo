force get-is-floating-panel
  field build, field ns-panel

  cause get-aspect
    drive object, share build
    drive aspect, |isFloatingPanel|
    raise build, field bool

force set-is-floating-panel
  field build, field ns-panel
  field value, field bool

  cause set-aspect
    drive object, share build
    drive aspect, |isFloatingPanel|
    drive factor, share value

