force get-location
  field build, field cl-placemark

  cause get-aspect
    drive object, share build
    drive aspect, |location|
    raise build, field cl-location

force set-location
  field build, field cl-placemark
  field value, field cl-location

  cause set-aspect
    drive object, share build
    drive aspect, |location|
    drive factor, share value

