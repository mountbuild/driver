force get-identifier
  field build, field un-notification-action

  cause get-aspect
    mount object, share build
    mount aspect, |identifier|
    raise build, field string

force set-identifier
  field build, field un-notification-action
  field value, field string

  cause set-aspect
    mount object, share build
    mount aspect, |identifier|
    mount factor, share value

force get-title
  field build, field un-notification-action

  cause get-aspect
    mount object, share build
    mount aspect, |title|
    raise build, field string

force set-title
  field build, field un-notification-action
  field value, field string

  cause set-aspect
    mount object, share build
    mount aspect, |title|
    mount factor, share value

force get-options
  field build, field un-notification-action

  cause get-aspect
    mount object, share build
    mount aspect, |options|
    raise build, field un-notification-action-options

force set-options
  field build, field un-notification-action
  field value, field un-notification-action-options

  cause set-aspect
    mount object, share build
    mount aspect, |options|
    mount factor, share value
