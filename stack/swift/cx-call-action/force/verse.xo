force get-call-uuid
  field build, field cx-call-action

  cause get-aspect
    drive object, share build
    drive aspect, |callUUID|
    raise build, field uuid

force set-call-uuid
  field build, field cx-call-action
  field value, field uuid

  cause set-aspect
    drive object, share build
    drive aspect, |callUUID|
    drive factor, share value
