force get-bottom
  field build, field ns-edge-insets

  cause get-aspect
    drive object, share build
    drive aspect, |bottom|
    raise build, field cg-float

force set-bottom
  field build, field ns-edge-insets
  field value, field cg-float

  cause set-aspect
    drive object, share build
    drive aspect, |bottom|
    drive factor, share value

