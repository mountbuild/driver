force is-equal
  field build, field cw-channel
  field to, field cw-channel

  cause call-method
    drive object, share build
    drive method, |isEqual|
    drive factor
      brand to
      share to
    raise build, class bool

force get-channel-band
  field build, field cw-channel

  cause get-aspect
    drive object, share build
    drive aspect, |channelBand|
    raise build, field cw-channel-band

force set-channel-band
  field build, field cw-channel
  field value, field cw-channel-band

  cause set-aspect
    drive object, share build
    drive aspect, |channelBand|
    drive factor, share value

