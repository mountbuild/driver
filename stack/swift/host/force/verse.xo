force get-address
  field build, field host

  cause get-aspect
    mount object, share build
    mount aspect, |address|
    raise build, field string

force set-address
  field build, field host
  field value, field string

  cause set-aspect
    mount object, share build
    mount aspect, |address|
    mount factor, share value

