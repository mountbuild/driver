force get-copy-description
  field build, field sc-network-reachability-context

  cause get-aspect
    drive object, share build
    drive aspect, |copyDescription|
    raise build, field unsafe-raw-pointer

force set-copy-description
  field build, field sc-network-reachability-context
  field value, field unsafe-raw-pointer

  cause set-aspect
    drive object, share build
    drive aspect, |copyDescription|
    drive factor, share value

