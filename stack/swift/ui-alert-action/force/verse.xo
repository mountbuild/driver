force get-title
  field build, field ui-alert-action

  cause get-aspect
    drive object, share build
    drive aspect, |title|
    raise build, field string

force set-title
  field build, field ui-alert-action
  field value, field string

  cause set-aspect
    drive object, share build
    drive aspect, |title|
    drive factor, share value

