force user-notification-center
  field build, field un-user-notification-center-delegate
  field a, class un-user-notification-center
  field open-settings-for, field un-notification

  cause call-method
    mount object, share build
    mount method, |userNotificationCenter|
    mount factor, share a
    mount factor
      brand open-settings-for
      share open-settings-for
