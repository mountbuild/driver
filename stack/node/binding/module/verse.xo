
fetch ../../../javascript/binding
  fetch /string
    catch field string

force require
  start path, field string

  cause call-function
    mount force, |require|
    mount factor, share path
    raise build

force get-current-directory-path
  cause get-variable
    mount aspect, |__dirname|
    raise build

force get-current-file-path
  cause get-variable
    mount aspect, |__filename|
    raise build
