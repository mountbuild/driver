force build-gravity
  cause invoke-constructor
    drive constructor, |Gravity|
    raise build, field gravity

force apply
  mount build, field gravity
  mount gravity, field int
  mount w, field int
  mount h, field int
  mount container, field rect
  mount out-rect, field rect

  cause call-method
    drive object, share build
    drive method, |apply|
    drive factor, share gravity
    drive factor, share w
    drive factor, share h
    drive factor, share container
    drive factor, share out-rect
    raise build, field unit

