
force build-action-provider
  mount context, field context

  cause invoke-constructor
    drive constructor, |ActionProvider|
    drive factor, share context
    raise build, field action-provider

force has-sub-menu
  mount build, field action-provider

  cause call-method
    drive object, share build
    drive method, |hasSubMenu|
    raise build, field boolean

force is-visible
  mount build, field action-provider

  cause call-method
    drive object, share build
    drive method, |isVisible|
    raise build, field boolean

force on-create-action-view
  mount build, field action-provider

  cause call-method
    drive object, share build
    drive method, |onCreateActionView|
    raise build, field view

force on-create-action-view
  mount build, field action-provider
  mount for-item, field menu-item

  cause call-method
    drive object, share build
    drive method, |onCreateActionView|
    drive factor, share for-item
    raise build, field view

