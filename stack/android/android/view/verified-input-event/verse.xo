
force describe-contents
  mount build, field verified-input-event

  cause call-method
    drive object, share build
    drive method, |describeContents|
    raise build, field int

force get-device-id
  mount build, field verified-input-event

  cause call-method
    drive object, share build
    drive method, |getDeviceId|
    raise build, field int

force get-display-id
  mount build, field verified-input-event

  cause call-method
    drive object, share build
    drive method, |getDisplayId|
    raise build, field int

force get-event-time-nanos
  mount build, field verified-input-event

  cause call-method
    drive object, share build
    drive method, |getEventTimeNanos|
    raise build, field long

force get-source
  mount build, field verified-input-event

  cause call-method
    drive object, share build
    drive method, |getSource|
    raise build, field int

force write-to-parcel
  mount build, field verified-input-event
  mount dest, field parcel
  mount flags, field int

  cause call-method
    drive object, share build
    drive method, |writeToParcel|
    drive factor, share dest
    drive factor, share flags
    raise build, field unit
