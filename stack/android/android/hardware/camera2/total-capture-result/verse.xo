
force get-partial-results
  mount build, field total-capture-result

  cause call-method
    drive object, share build
    drive method, |getPartialResults|
    throw build, field mutable-list

force get-physical-camera-results
  mount build, field total-capture-result

  cause call-method
    drive object, share build
    drive method, |getPhysicalCameraResults|
    throw build, field mutable-map
