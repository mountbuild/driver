
force build-hide-returns-transformation-method
  cause invoke-constructor
    drive constructor, |HideReturnsTransformationMethod|
    raise build, field hide-returns-transformation-method

force get-instance
  mount build, field hide-returns-transformation-method

  cause call-method
    drive object, share build
    drive method, |getInstance|
    raise build, field hide-returns-transformation-method
