
force get
  mount build, field auto-text
  mount src, field char-sequence
  mount start, field int
  mount end, field int
  mount view, field view

  cause call-method
    drive object, share build
    drive method, |get|
    drive factor, share src
    drive factor, share start
    drive factor, share end
    drive factor, share view
    raise build, field string

force get-size
  mount build, field auto-text
  mount view, field view

  cause call-method
    drive object, share build
    drive method, |getSize|
    drive factor, share view
    raise build, field int
