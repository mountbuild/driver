
force describe-contents
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |describeContents|
    raise build, field int

force get-card-icon
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getCardIcon|
    raise build, field icon

force get-card-id
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getCardId|
    raise build, field string

force get-card-image
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getCardImage|
    raise build, field icon

force get-card-label
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getCardLabel|
    raise build, field char-sequence

force get-content-description
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getContentDescription|
    raise build, field char-sequence

force get-pending-intent
  mount build, field wallet-card

  cause call-method
    drive object, share build
    drive method, |getPendingIntent|
    raise build, field pending-intent

force write-to-parcel
  mount build, field wallet-card
  mount dest, field parcel
  mount flags, field int

  cause call-method
    drive object, share build
    drive method, |writeToParcel|
    drive factor, share dest
    drive factor, share flags
    raise build, field unit
