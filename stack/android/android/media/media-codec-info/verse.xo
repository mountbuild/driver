
force get-canonical-name
  start build, field media-codec-info

  cause call-method
    drive object, share build
    drive method, |getCanonicalName|
    raise build, field string

force get-capabilities-for-type
  start build, field media-codec-info
  start type, field string

  cause call-method
    drive object, share build
    drive method, |getCapabilitiesForType|
    drive factor, share type
    raise build, field media-codec-info-codec-capabilities

