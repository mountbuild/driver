
force close-sessions
  mount build, field reader

  cause call-method
    drive object, share build
    drive method, |closeSessions|
    raise build, field unit

force get-name
  mount build, field reader

  cause call-method
    drive object, share build
    drive method, |getName|
    raise build, field string

force get-se-service
  mount build, field reader

  cause call-method
    drive object, share build
    drive method, |getSEService|
    raise build, field se-service

force is-secure-element-present
  mount build, field reader

  cause call-method
    drive object, share build
    drive method, |isSecureElementPresent|
    raise build, field boolean

force open-session
  mount build, field reader

  cause call-method
    drive object, share build
    drive method, |openSession|
    raise build, field session
