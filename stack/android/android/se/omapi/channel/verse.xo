
force close
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |close|
    raise build, field unit

force get-select-response
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |getSelectResponse|
    raise build, field byte-array

force get-session
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |getSession|
    raise build, field session

force is-basic-channel
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |isBasicChannel|
    raise build, field boolean

force is-open
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |isOpen|
    raise build, field boolean

force select-next
  mount build, field channel

  cause call-method
    drive object, share build
    drive method, |selectNext|
    raise build, field boolean

force transmit
  mount build, field channel
  mount command, field byte-array

  cause call-method
    drive object, share build
    drive method, |transmit|
    drive factor, share command
    raise build, field byte-array
