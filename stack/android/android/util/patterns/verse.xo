
force get-good-iri-char
  mount build, field patterns

  cause get-property
    drive object, share build
    drive property, |GOOD_IRI_CHAR|
    throw build, field string

force get-top-level-domain-str
  mount build, field patterns

  cause get-property
    drive object, share build
    drive property, |TOP_LEVEL_DOMAIN_STR|
    throw build, field string

force get-top-level-domain-str-for-web-url
  mount build, field patterns

  cause get-property
