
force get-quota
  start build, field full-backup-data-output

  cause call-method
    drive object, share build
    drive method, |getQuota|
    throw build, field long

force get-transport-flags
  start build, field full-backup-data-output

  cause call-method
    drive object, share build
    drive method, |getTransportFlags|
    throw build, field int
