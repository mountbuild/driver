force add-completed-download
  start build, field download-manager
  start title, field string
  start description, field string
  start is-media-scanner-scannable, field boolean
  start mime-type, field string
  start path, field string
  start length, field long
  start show-notification, field boolean

  cause call-method
    drive object, share build
    drive method, |addCompletedDownload|
    drive factor, share title
    drive factor, share description
    drive factor, share is-media-scanner-scannable
    drive factor, share mime-type
    drive factor, share path
    drive factor, share length
    drive factor, share show-notification
    throw build, field long

