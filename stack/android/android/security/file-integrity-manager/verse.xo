
force is-apk-verity-supported
  mount build, field file-integrity-manager

  cause call-method
    drive object, share build
    drive method, |isApkVeritySupported|
    throw build, field boolean

force is-app-source-certificate-trusted
  mount build, field file-integrity-manager
  mount certificate, field x509-certificate

  cause call-method
    drive object, share build
    drive method, |isAppSourceCertificateTrusted|
    drive factor, share certificate
    throw build, field boolean
