
force build-loader-test-case
  cause invoke-constructor
    drive constructor, |LoaderTestCase|
    raise build, field loader-test-case

force get-loader-result-synchronously
  mount build, field loader-test-case
  mount loader, field loader-t

  cause call-method
    drive object, share build
    drive method, |getLoaderResultSynchronously|
    drive factor, share loader
    raise build, field t
