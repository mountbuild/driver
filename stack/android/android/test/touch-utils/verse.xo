
force build-touch-utils
  cause invoke-constructor
    drive constructor, |TouchUtils|
    raise build, field touch-utils

force click-view
  mount build, field touch-utils
  mount test, field instrumentation-test-case
  mount v, field view

  cause call-method
    drive object, share build
    drive method, |clickView|
    drive factor, share test
    drive factor, share v
    raise build, field unit

force drag
  mount build, field touch-utils
  mount test, field activity-instrumentation-test-case-activity
  mount from-x, field float
  mount to-x, field float
  mount from-y, field float
  mount to-y, field float
  mount step-count, field int

  cause call-method
    drive object, share build
    drive method, |drag|
    drive factor, share test
    drive factor, share from-x
    drive factor, share to-x
    drive factor, share from-y
    drive factor, share to-y
    drive factor, share step-count
    raise build, field unit

