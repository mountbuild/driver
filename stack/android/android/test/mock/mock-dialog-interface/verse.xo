
force build-mock-dialog-interface
  cause invoke-constructor
    drive constructor, |MockDialogInterface|
    raise build, field mock-dialog-interface

force cancel
  mount build, field mock-dialog-interface

  cause call-method
    drive object, share build
    drive method, |cancel|
    raise build, field unit

force dismiss
  mount build, field mock-dialog-interface

  cause call-method
    drive object, share build
    drive method, |dismiss|
    raise build, field unit
