
force build-mock-resources
  cause invoke-constructor
    drive constructor, |MockResources|
    raise build, field mock-resources

force get-animation
  mount build, field mock-resources
  mount id, field int

  cause call-method
    drive object, share build
    drive method, |getAnimation|
    drive factor, share id
    raise build, field xml-resource-parser

