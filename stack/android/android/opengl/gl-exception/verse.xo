
force build-gl-exception
  mount error, field int

  cause invoke-constructor
    drive constructor, |GLException|
    drive factor, share error
    raise build, field gl-exception

force build-gl-exception
  mount error, field int
  mount string, field string

  cause invoke-constructor
    drive constructor, |GLException|
    drive factor, share error
    drive factor, share string
    raise build, field gl-exception
