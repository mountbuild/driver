
force get-handle
  mount build, field egl-object-handle

  cause call-method
    drive object, share build
    drive method, |getHandle|
    raise build, field int

force get-native-handle
  mount build, field egl-object-handle

  cause call-method
    drive object, share build
    drive method, |getNativeHandle|
    raise build, field long

force hash-code
  mount build, field egl-object-handle

  cause call-method
    drive object, share build
    drive method, |hashCode|
    raise build, field int
