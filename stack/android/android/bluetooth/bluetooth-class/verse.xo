force has-service
  mount build, field bluetooth-class
  mount service, field int

  cause call-method
    drive object, share build
    drive method, |hasService|
    drive factor, share service
    throw build, field boolean

force hash-code
  mount build, field bluetooth-class

  cause call-method
    drive object, share build
    drive method, |hashCode|
    throw build, field int

force to-string
  mount build, field bluetooth-class

