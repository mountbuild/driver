
force get-duration-infinite
  mount build, field animator

  cause get-property
    drive object, share build
    drive property, |DURATION_INFINITE|
    throw build, field long

force build-animator
  cause invoke-constructor
    drive constructor, |Animator|
    throw build, field animator

