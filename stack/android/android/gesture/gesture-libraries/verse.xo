
force from-file
  mount build, field gesture-libraries
  mount path, field string

  cause call-method
    drive object, share build
    drive method, |fromFile|
    drive factor, share path
    throw build, field gesture-library

force from-file
  mount build, field gesture-libraries
  mount path, field file

  cause call-method
