
force get-transport-type-wlan
  mount build, field access-network-constants

  cause get-property
    drive object, share build
    drive property, |TRANSPORT_TYPE_WLAN|
    throw build, field int

force get-transport-type-wwan
  mount build, field access-network-constants

  cause get-property
    drive object, share build
    drive property, |TRANSPORT_TYPE_WWAN|
    throw build, field int
