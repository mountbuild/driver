
force get-code-error-invalid-subscription
  mount build, field ims-exception

  cause get-property
    drive object, share build
    drive property, |CODE_ERROR_INVALID_SUBSCRIPTION|
    throw build, field int

force get-code-error-service-unavailable
  mount build, field ims-exception

  cause get-property
    drive object, share build
    drive property, |CODE_ERROR_SERVICE_UNAVAILABLE|
    throw build, field int

force get-code-error-unspecified
  mount build, field ims-exception

  cause get-property
    drive object, share build
    drive property, |CODE_ERROR_UNSPECIFIED|
    throw build, field int

force get-code-error-unsupported-operation
  mount build, field ims-exception

  cause get-property
    drive object, share build
    drive property, |CODE_ERROR_UNSUPPORTED_OPERATION|
    throw build, field int

force get-code
  mount build, field ims-exception

  cause call-method
    drive object, share build
    drive method, |getCode|
    throw build, field int
