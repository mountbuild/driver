
force build-cell-location
  cause invoke-constructor
    drive constructor, |CellLocation|
    throw build, field cell-location

force get-empty
  mount build, field cell-location

  cause call-method
    drive object, share build
    drive method, |getEmpty|
    throw build, field cell-location

force request-location-update
  mount build, field cell-location

  cause call-method
    drive object, share build
    drive method, |requestLocationUpdate|
    throw build, field unit
