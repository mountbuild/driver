
force format
  mount build, field list-formatter
  mount vararg-items, field any

  cause call-method
    drive object, share build
    drive method, |format|
    drive factor, share vararg-items
    raise build, field string

force format
  mount build, field list-formatter
  mount items, field mutable-collection

  cause call-method
    drive object, share build
    drive method, |format|
    drive factor, share items
    raise build, field string

