force get-uts46-instance
  start build, field idna
  start options, field int

  cause call-method
    drive object, share build
    drive method, |getUTS46Instance|
    drive factor, share options
    raise build, field idna

force label-to-ascii
  start build, field idna
  start label, field char-sequence
  start dest, field string-builder
  start info, field idna-info

  cause call-method
    drive object, share build
    drive method, |labelToASCII|
    drive factor, share label
    drive factor, share dest
    drive factor, share info
    raise build, field string-builder

