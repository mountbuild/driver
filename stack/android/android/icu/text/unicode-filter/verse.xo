
force contains
  start build, field unicode-filter
  start c, field int

  cause call-method
    drive object, share build
    drive method, |contains|
    drive factor, share c
    raise build, field boolean

force matches
  start build, field unicode-filter
  start text, field replaceable
  start offset, field int-array
  start limit, field int
  start incremental, field boolean

  cause call-method
    drive object, share build
    drive method, |matches|
    drive factor, share text
    drive factor, share offset
    drive factor, share limit
    drive factor, share incremental
    raise build, field int
