
force to-big-decimal
  mount build, field formatted-number

  cause call-method
    drive object, share build
    drive method, |toBigDecimal|
    raise build, field big-decimal

force to-character-iterator
  mount build, field formatted-number

  cause call-method
    drive object, share build
    drive method, |toCharacterIterator|
    raise build, field attributed-character-iterator

force to-string
  mount build, field formatted-number

  cause call-method
    drive object, share build
    drive method, |toString|
    raise build, field string
