
force with
  mount build, field number-formatter

  cause call-method
    drive object, share build
    drive method, |with|
    raise build, field unlocalized-number-formatter

force with-locale
  mount build, field number-formatter
  mount locale, field locale

  cause call-method
    drive object, share build
    drive method, |withLocale|
    drive factor, share locale
    raise build, field localized-number-formatter

force with-locale
  mount build, field number-formatter
  mount locale, field u-locale

  cause call-method
    drive object, share build
    drive method, |withLocale|
    drive factor, share locale
    raise build, field localized-number-formatter
