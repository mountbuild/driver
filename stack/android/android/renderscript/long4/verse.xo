
force build-long4
  cause invoke-constructor
    drive constructor, |Long4|
    raise build, field long4

force build-long4
  mount x, field long
  mount y, field long
  mount z, field long
  mount w, field long

  cause invoke-constructor
    drive constructor, |Long4|
    drive factor, share x
    drive factor, share y
    drive factor, share z
    drive factor, share w
    raise build, field long4
