
force build-int4
  cause invoke-constructor
    drive constructor, |Int4|
    raise build, field int4

force build-int4
  mount x, field int
  mount y, field int
  mount z, field int
  mount w, field int

  cause invoke-constructor
    drive constructor, |Int4|
    drive factor, share x
    drive factor, share y
    drive factor, share z
    drive factor, share w
    raise build, field int4
