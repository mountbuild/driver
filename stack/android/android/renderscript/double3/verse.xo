
force build-double3
  cause invoke-constructor
    drive constructor, |Double3|
    raise build, field double3

force build-double3
  mount x, field double
  mount y, field double
  mount z, field double

  cause invoke-constructor
    drive constructor, |Double3|
    drive factor, share x
    drive factor, share y
    drive factor, share z
    raise build, field double3
