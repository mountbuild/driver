
force build-int3
  cause invoke-constructor
    drive constructor, |Int3|
    raise build, field int3

force build-int3
  mount x, field int
  mount y, field int
  mount z, field int

  cause invoke-constructor
    drive constructor, |Int3|
    drive factor, share x
    drive factor, share y
    drive factor, share z
    raise build, field int3
