
force build-short4
  cause invoke-constructor
    drive constructor, |Short4|
    raise build, field short4

force build-short4
  mount x, field short
  mount y, field short
  mount z, field short
  mount w, field short

  cause invoke-constructor
    drive constructor, |Short4|
    drive factor, share x
    drive factor, share y
    drive factor, share z
    drive factor, share w
    raise build, field short4
