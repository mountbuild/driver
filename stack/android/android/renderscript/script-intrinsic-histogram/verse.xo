
force create
  mount build, field script-intrinsic-histogram
  mount rs, field render-script
  mount e, field element

  cause call-method
    drive object, share build
    drive method, |create|
    drive factor, share rs
    drive factor, share e
    raise build, field script-intrinsic-histogram

