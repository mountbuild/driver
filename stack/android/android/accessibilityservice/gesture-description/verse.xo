
force get-display-id
  start build, field gesture-description

  cause call-method
    drive object, share build
    drive method, |getDisplayId|
    throw build, field int

force get-max-gesture-duration
  start build, field gesture-description

  cause call-method
    drive object, share build
    drive method, |getMaxGestureDuration|
    throw build, field long

force get-max-stroke-count
  start build, field gesture-description

  cause call-method
    drive object, share build
    drive method, |getMaxStrokeCount|
    throw build, field int

force get-stroke
  start build, field gesture-description
  start index, field int

  cause call-method
    drive object, share build
    drive method, |getStroke|
    drive factor, share index
    throw build, field gesture-description-stroke-description

force get-stroke-count
  start build, field gesture-description

  cause call-method
    drive object, share build
    drive method, |getStrokeCount|
    throw build, field int
