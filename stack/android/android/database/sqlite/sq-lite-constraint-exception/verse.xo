
force build-sq-lite-constraint-exception
  cause invoke-constructor
    drive constructor, |SQLiteConstraintException|
    raise build, field sq-lite-constraint-exception

force build-sq-lite-constraint-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteConstraintException|
    drive factor, share error
    raise build, field sq-lite-constraint-exception
