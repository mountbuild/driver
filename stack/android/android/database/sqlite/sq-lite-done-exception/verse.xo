
force build-sq-lite-done-exception
  cause invoke-constructor
    drive constructor, |SQLiteDoneException|
    raise build, field sq-lite-done-exception

force build-sq-lite-done-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteDoneException|
    drive factor, share error
    raise build, field sq-lite-done-exception
