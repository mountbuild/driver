
force build-sq-lite-out-of-memory-exception
  cause invoke-constructor
    drive constructor, |SQLiteOutOfMemoryException|
    raise build, field sq-lite-out-of-memory-exception

force build-sq-lite-out-of-memory-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteOutOfMemoryException|
    drive factor, share error
    raise build, field sq-lite-out-of-memory-exception
