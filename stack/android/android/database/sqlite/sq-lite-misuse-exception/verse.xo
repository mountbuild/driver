
force build-sq-lite-misuse-exception
  cause invoke-constructor
    drive constructor, |SQLiteMisuseException|
    raise build, field sq-lite-misuse-exception

force build-sq-lite-misuse-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteMisuseException|
    drive factor, share error
    raise build, field sq-lite-misuse-exception
