
force build-sq-lite-bind-or-column-index-out-of-range-exception
  cause invoke-constructor
    drive constructor, |SQLiteBindOrColumnIndexOutOfRangeException|
    raise build, field sq-lite-bind-or-column-index-out-of-range-exception

force build-sq-lite-bind-or-column-index-out-of-range-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteBindOrColumnIndexOutOfRangeException|
    drive factor, share error
    raise build, field sq-lite-bind-or-column-index-out-of-range-exception
