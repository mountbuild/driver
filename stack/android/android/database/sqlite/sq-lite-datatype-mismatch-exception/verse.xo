
force build-sq-lite-datatype-mismatch-exception
  cause invoke-constructor
    drive constructor, |SQLiteDatatypeMismatchException|
    raise build, field sq-lite-datatype-mismatch-exception

force build-sq-lite-datatype-mismatch-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteDatatypeMismatchException|
    drive factor, share error
    raise build, field sq-lite-datatype-mismatch-exception
