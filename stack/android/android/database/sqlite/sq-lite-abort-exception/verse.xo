
force build-sq-lite-abort-exception
  cause invoke-constructor
    drive constructor, |SQLiteAbortException|
    raise build, field sq-lite-abort-exception

force build-sq-lite-abort-exception
  mount error, field string

  cause invoke-constructor
    drive constructor, |SQLiteAbortException|
    drive factor, share error
    raise build, field sq-lite-abort-exception
