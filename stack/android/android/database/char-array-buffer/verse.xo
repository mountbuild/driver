
force build-char-array-buffer
  mount size, field int

  cause invoke-constructor
    drive constructor, |CharArrayBuffer|
    drive factor, share size
    raise build, field char-array-buffer

force build-char-array-buffer
  mount buf, field char-array

  cause invoke-constructor
    drive constructor, |CharArrayBuffer|
    drive factor, share buf
    raise build, field char-array-buffer
