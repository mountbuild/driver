
force build-radial-gradient
  mount center-x, field float
  mount center-y, field float
  mount radius, field float
  mount colors, field int-array
  mount stops, field float-array
  mount tile-mode, field shader-tile-mode

  cause invoke-constructor
    drive constructor, |RadialGradient|
    drive factor, share center-x
    drive factor, share center-y
    drive factor, share radius
    drive factor, share colors
    drive factor, share stops
    drive factor, share tile-mode
    raise build, field radial-gradient

