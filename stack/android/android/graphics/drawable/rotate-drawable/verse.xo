
force build-rotate-drawable
  cause invoke-constructor
    drive constructor, |RotateDrawable|
    raise build, field rotate-drawable

force apply-theme
  mount build, field rotate-drawable
  mount t, field resources-theme

  cause call-method
    drive object, share build
    drive method, |applyTheme|
    drive factor, share t
    raise build, field unit

