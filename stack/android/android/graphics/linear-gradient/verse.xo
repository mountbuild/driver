
force build-linear-gradient
  mount x0, field float
  mount y0, field float
  mount x1, field float
  mount y1, field float
  mount colors, field int-array
  mount positions, field float-array
  mount tile, field shader-tile-mode

  cause invoke-constructor
    drive constructor, |LinearGradient|
    drive factor, share x0
    drive factor, share y0
    drive factor, share x1
    drive factor, share y1
    drive factor, share colors
    drive factor, share positions
    drive factor, share tile
    raise build, field linear-gradient

