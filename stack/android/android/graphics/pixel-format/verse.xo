force build-pixel-format
  cause invoke-constructor
    drive constructor, |PixelFormat|
    raise build, field pixel-format

force format-has-alpha
  mount build, field pixel-format
  mount format, field int

  cause call-method
    drive object, share build
    drive method, |formatHasAlpha|
    drive factor, share format
    raise build, field boolean

force get-pixel-format-info
  mount build, field pixel-format
  mount format, field int
  mount info, field pixel-format

  cause call-method
    drive object, share build
    drive method, |getPixelFormatInfo|
    drive factor, share format
    drive factor, share info
    raise build, field unit
