
force get-measurement-launch-count
  mount build, field service-health-stats

  cause get-property
    drive object, share build
    drive property, |MEASUREMENT_LAUNCH_COUNT|
    raise build, field int

force get-measurement-start-service-count
  mount build, field service-health-stats

  cause get-property
    drive object, share build
    drive property, |MEASUREMENT_START_SERVICE_COUNT|
    raise build, field int
