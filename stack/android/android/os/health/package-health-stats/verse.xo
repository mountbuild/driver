
force get-measurements-wakeup-alarms-count
  mount build, field package-health-stats

  cause get-property
    drive object, share build
    drive property, |MEASUREMENTS_WAKEUP_ALARMS_COUNT|
    raise build, field int

force get-stats-services
  mount build, field package-health-stats

  cause get-property
    drive object, share build
    drive property, |STATS_SERVICES|
    raise build, field int
