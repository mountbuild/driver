
force get-unknown
  mount build, field build

  cause get-property
    drive object, share build
    drive property, |UNKNOWN|
    raise build, field string

force build-build
  cause invoke-constructor
    drive constructor, |Build|
    raise build, field build

force get-fingerprinted-partitions
  mount build, field build

  cause call-method
    drive object, share build
    drive method, |getFingerprintedPartitions|
    raise build, field mutable-list

force get-radio-version
  mount build, field build

  cause call-method
    drive object, share build
    drive method, |getRadioVersion|
    raise build, field string

force get-serial
  mount build, field build

  cause call-method
    drive object, share build
    drive method, |getSerial|
    raise build, field string
