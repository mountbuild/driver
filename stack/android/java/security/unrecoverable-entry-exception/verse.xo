
force build-unrecoverable-entry-exception
  cause invoke-constructor
    mount constructor, |UnrecoverableEntryException|
    throw build, field unrecoverable-entry-exception

force build-unrecoverable-entry-exception
  mount msg, field string

  cause invoke-constructor
    mount constructor, |UnrecoverableEntryException|
    mount factor, share msg
    throw build, field unrecoverable-entry-exception
