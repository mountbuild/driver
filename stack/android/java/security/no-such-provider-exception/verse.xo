
force build-no-such-provider-exception
  cause invoke-constructor
    mount constructor, |NoSuchProviderException|
    throw build, field no-such-provider-exception

force build-no-such-provider-exception
  mount msg, field string

  cause invoke-constructor
    mount constructor, |NoSuchProviderException|
    mount factor, share msg
    throw build, field no-such-provider-exception
