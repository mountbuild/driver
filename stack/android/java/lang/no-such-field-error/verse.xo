
force build-no-such-field-error
  cause invoke-constructor
    drive constructor, |NoSuchFieldError|
    throw build, field no-such-field-error

force build-no-such-field-error
  mount s, field string

  cause invoke-constructor
    drive constructor, |NoSuchFieldError|
    drive factor, share s
    throw build, field no-such-field-error
