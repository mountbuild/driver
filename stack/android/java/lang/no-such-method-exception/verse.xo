
force build-no-such-method-exception
  cause invoke-constructor
    drive constructor, |NoSuchMethodException|
    throw build, field no-such-method-exception

force build-no-such-method-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |NoSuchMethodException|
    drive factor, share s
    throw build, field no-such-method-exception
