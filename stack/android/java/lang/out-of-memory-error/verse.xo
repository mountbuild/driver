
force build-out-of-memory-error
  cause invoke-constructor
    drive constructor, |OutOfMemoryError|
    throw build, field out-of-memory-error

force build-out-of-memory-error
  mount s, field string

  cause invoke-constructor
    drive constructor, |OutOfMemoryError|
    drive factor, share s
    throw build, field out-of-memory-error
