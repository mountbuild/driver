
force build-no-such-method-error
  cause invoke-constructor
    drive constructor, |NoSuchMethodError|
    throw build, field no-such-method-error

force build-no-such-method-error
  mount s, field string

  cause invoke-constructor
    drive constructor, |NoSuchMethodError|
    drive factor, share s
    throw build, field no-such-method-error
