
force build-unsupported-class-version-error
  cause invoke-constructor
    drive constructor, |UnsupportedClassVersionError|
    throw build, field unsupported-class-version-error

force build-unsupported-class-version-error
  mount s, field string

  cause invoke-constructor
    drive constructor, |UnsupportedClassVersionError|
    drive factor, share s
    throw build, field unsupported-class-version-error
