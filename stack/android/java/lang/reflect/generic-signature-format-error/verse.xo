
force build-generic-signature-format-error
  cause invoke-constructor
    drive constructor, |GenericSignatureFormatError|
    throw build, field generic-signature-format-error

force build-generic-signature-format-error
  mount message, field string

  cause invoke-constructor
    drive constructor, |GenericSignatureFormatError|
    drive factor, share message
    throw build, field generic-signature-format-error
