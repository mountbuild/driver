
force build-negative-array-size-exception
  cause invoke-constructor
    drive constructor, |NegativeArraySizeException|
    throw build, field negative-array-size-exception

force build-negative-array-size-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |NegativeArraySizeException|
    drive factor, share s
    throw build, field negative-array-size-exception
