
force build-no-such-field-exception
  cause invoke-constructor
    drive constructor, |NoSuchFieldException|
    throw build, field no-such-field-exception

force build-no-such-field-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |NoSuchFieldException|
    drive factor, share s
    throw build, field no-such-field-exception
