
force build-unsupported-encoding-exception
  cause invoke-constructor
    drive constructor, |UnsupportedEncodingException|
    throw build, field unsupported-encoding-exception

force build-unsupported-encoding-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |UnsupportedEncodingException|
    drive factor, share s
    throw build, field unsupported-encoding-exception
