
force flush
  mount build, field console

  cause call-method
    drive object, share build
    drive method, |flush|
    throw build, field unit

force format
  mount build, field console
  mount fmt, field string
  mount vararg-args, field any

  cause call-method
    drive object, share build
    drive method, |format|
    drive factor, share fmt
    drive factor, share vararg-args
    throw build, field console

force printf
