
force for-class
  mount build, field object-stream-class

  cause call-method
    drive object, share build
    drive method, |forClass|
    throw build, field class

force get-field
  mount build, field object-stream-class
  mount name, field string

  cause call-method
    drive object, share build
    drive method, |getField|
    drive factor, share name
    throw build, field object-stream-field

force get-fields
  mount build, field object-stream-class

  cause call-method
    drive object, share build
    drive method, |getFields|
    throw build, field array

force get-name
  mount build, field object-stream-class

  cause call-method
