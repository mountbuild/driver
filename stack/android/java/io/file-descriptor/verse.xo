
force build-file-descriptor
  cause invoke-constructor
    drive constructor, |FileDescriptor|
    throw build, field file-descriptor

force sync
  mount build, field file-descriptor

  cause call-method
    drive object, share build
    drive method, |sync|
    throw build, field unit

force valid
  mount build, field file-descriptor

  cause call-method
    drive object, share build
    drive method, |valid|
    throw build, field boolean
