
force clone
  start build, field format

  cause call-method
    mount object, share build
    mount method, |clone|
    raise build, field any

force format
  start build, field format
  start obj, field any

  cause call-method
    mount object, share build
    mount method, |format|
    mount factor, share obj
    raise build, field string

force format
  start build, field format
  start obj, field any
  start to-append-to, field string-buffer
  start pos, field field-position

  cause call-method
    mount object, share build
    mount method, |format|
    mount factor, share obj
    mount factor, share to-append-to
    mount factor, share pos
    raise build, field string-buffer

force format-to-character-iterator
  start build, field format
  start obj, field any

  cause call-method
    mount object, share build
    mount method, |formatToCharacterIterator|
    mount factor, share obj
    raise build, field attributed-character-iterator

force parse-object
  start build, field format
  start source, field string
  start pos, field parse-position

  cause call-method
    mount object, share build
    mount method, |parseObject|
    mount factor, share source
    mount factor, share pos
    raise build, field any

force parse-object
  start build, field format
  start source, field string

  cause call-method
    mount object, share build
    mount method, |parseObject|
    mount factor, share source
    raise build, field any
