
force is-normalized
  start build, field normalizer
  start src, field char-sequence
  start form, field normalizer-form

  cause call-method
    mount object, share build
    mount method, |isNormalized|
    mount factor, share src
    mount factor, share form
    raise build, field boolean

force normalize
  start build, field normalizer
  start src, field char-sequence
  start form, field normalizer-form

  cause call-method
    mount object, share build
    mount method, |normalize|
    mount factor, share src
    mount factor, share form
    raise build, field string
