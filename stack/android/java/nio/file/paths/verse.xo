
force get
  mount build, field paths
  mount first, field string
  mount vararg-more, field string

  cause call-method
    drive object, share build
    drive method, |get|
    drive factor, share first
    drive factor, share vararg-more
    throw build, field path

force get
  mount build, field paths
  mount uri, field uri

  cause call-method
    drive object, share build
    drive method, |get|
    drive factor, share uri
    throw build, field path
