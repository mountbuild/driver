
force force
  mount build, field mapped-byte-buffer

  cause call-method
    mount object, share build
    mount method, |force|
    throw build, field mapped-byte-buffer

force is-loaded
  mount build, field mapped-byte-buffer

  cause call-method
    mount object, share build
    mount method, |isLoaded|
    throw build, field boolean

force load
  mount build, field mapped-byte-buffer

  cause call-method
    mount object, share build
    mount method, |load|
    throw build, field mapped-byte-buffer
