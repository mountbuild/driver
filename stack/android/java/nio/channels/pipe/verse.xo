
force open
  mount build, field pipe

  cause call-method
    mount object, share build
    mount method, |open|
    throw build, field pipe

force sink
  mount build, field pipe

  cause call-method
    mount object, share build
    mount method, |sink|
    throw build, field pipe-sink-channel

force source
  mount build, field pipe

  cause call-method
    mount object, share build
    mount method, |source|
    throw build, field pipe-source-channel
