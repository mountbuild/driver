
force close
  mount build, field abstract-selector

  cause call-method
    mount object, share build
    mount method, |close|
    throw build, field unit

force is-open
  mount build, field abstract-selector

  cause call-method
    mount object, share build
    mount method, |isOpen|
    throw build, field boolean

force provider
  mount build, field abstract-selector

  cause call-method
    mount object, share build
    mount method, |provider|
    throw build, field selector-provider
