
force build-invalid-properties-format-exception
  mount cause, field throwable

  cause invoke-constructor
    drive constructor, |InvalidPropertiesFormatException|
    drive factor, share cause
    throw build, field invalid-properties-format-exception

force build-invalid-properties-format-exception
  mount message, field string

  cause invoke-constructor
    drive constructor, |InvalidPropertiesFormatException|
    drive factor, share message
    throw build, field invalid-properties-format-exception
