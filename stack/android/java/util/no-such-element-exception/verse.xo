
force build-no-such-element-exception
  cause invoke-constructor
    drive constructor, |NoSuchElementException|
    throw build, field no-such-element-exception

force build-no-such-element-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |NoSuchElementException|
    drive factor, share s
    throw build, field no-such-element-exception
