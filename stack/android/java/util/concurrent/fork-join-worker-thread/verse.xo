
force get-pool
  mount build, field fork-join-worker-thread

  cause call-method
    drive object, share build
    drive method, |getPool|
    throw build, field fork-join-pool

force get-pool-index
  mount build, field fork-join-worker-thread

  cause call-method
    drive object, share build
    drive method, |getPoolIndex|
    throw build, field int

force run
  mount build, field fork-join-worker-thread

  cause call-method
    drive object, share build
    drive method, |run|
    throw build, field unit
