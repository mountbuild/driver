
force cancel
  mount build, field timer-task

  cause call-method
    drive object, share build
    drive method, |cancel|
    throw build, field boolean

force run
  mount build, field timer-task

  cause call-method
    drive object, share build
    drive method, |run|
    throw build, field unit

force scheduled-execution-time
  mount build, field timer-task

  cause call-method
    drive object, share build
    drive method, |scheduledExecutionTime|
    throw build, field long
