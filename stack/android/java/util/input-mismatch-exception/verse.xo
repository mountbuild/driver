
force build-input-mismatch-exception
  cause invoke-constructor
    drive constructor, |InputMismatchException|
    throw build, field input-mismatch-exception

force build-input-mismatch-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |InputMismatchException|
    drive factor, share s
    throw build, field input-mismatch-exception
