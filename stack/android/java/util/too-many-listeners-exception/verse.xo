
force build-too-many-listeners-exception
  cause invoke-constructor
    drive constructor, |TooManyListenersException|
    throw build, field too-many-listeners-exception

force build-too-many-listeners-exception
  mount s, field string

  cause invoke-constructor
    drive constructor, |TooManyListenersException|
    drive factor, share s
    throw build, field too-many-listeners-exception
