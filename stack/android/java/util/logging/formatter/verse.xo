
force format
  mount build, field formatter
  mount record, field log-record

  cause call-method
    drive object, share build
    drive method, |format|
    drive factor, share record
    throw build, field string

force format-message
  mount build, field formatter
  mount record, field log-record

  cause call-method
    drive object, share build
    drive method, |formatMessage|
    drive factor, share record
    throw build, field string

force get-head
  mount build, field formatter
  mount h, field handler

  cause call-method
    drive object, share build
    drive method, |getHead|
    drive factor, share h
    throw build, field string

force get-tail
  mount build, field formatter
  mount h, field handler

  cause call-method
    drive object, share build
    drive method, |getTail|
    drive factor, share h
    throw build, field string
