
force build-no-such-padding-exception
  cause invoke-constructor
    mount constructor, |NoSuchPaddingException|
    throw build, field no-such-padding-exception

force build-no-such-padding-exception
  start msg, field string

  cause invoke-constructor
    mount constructor, |NoSuchPaddingException|
    mount factor, share msg
    throw build, field no-such-padding-exception
