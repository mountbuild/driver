
force build-illegal-block-size-exception
  cause invoke-constructor
    mount constructor, |IllegalBlockSizeException|
    throw build, field illegal-block-size-exception

force build-illegal-block-size-exception
  start msg, field string

  cause invoke-constructor
    mount constructor, |IllegalBlockSizeException|
    mount factor, share msg
    throw build, field illegal-block-size-exception
