
force build-x-path-factory-configuration-exception
  start message, field string

  cause invoke-constructor
    mount constructor, |XPathFactoryConfigurationException|
    mount factor, share message
    throw build, field x-path-factory-configuration-exception

force build-x-path-factory-configuration-exception
  start cause, field throwable

  cause invoke-constructor
    mount constructor, |XPathFactoryConfigurationException|
    mount factor, share cause
    throw build, field x-path-factory-configuration-exception
