
force build-x-path-expression-exception
  start message, field string

  cause invoke-constructor
    mount constructor, |XPathExpressionException|
    mount factor, share message
    throw build, field x-path-expression-exception

force build-x-path-expression-exception
  start cause, field throwable

  cause invoke-constructor
    mount constructor, |XPathExpressionException|
    mount factor, share cause
    throw build, field x-path-expression-exception
