
force make-parser
  mount build, field parser-factory

  cause call-method
    drive object, share build
    drive method, |makeParser|
    throw build, field parser

force make-parser
  mount build, field parser-factory
  mount class-name, field string

  cause call-method
    drive object, share build
    drive method, |makeParser|
    drive factor, share class-name
    throw build, field parser
