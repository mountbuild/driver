
force create-xml-reader
  mount build, field xml-reader-factory

  cause call-method
    drive object, share build
    drive method, |createXMLReader|
    throw build, field xml-reader

force create-xml-reader
  mount build, field xml-reader-factory
  mount class-name, field string

  cause call-method
    drive object, share build
    drive method, |createXMLReader|
    drive factor, share class-name
    throw build, field xml-reader
