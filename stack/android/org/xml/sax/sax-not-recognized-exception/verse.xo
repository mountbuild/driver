
force build-sax-not-recognized-exception
  cause invoke-constructor
    drive constructor, |SAXNotRecognizedException|
    throw build, field sax-not-recognized-exception

force build-sax-not-recognized-exception
  mount message, field string

  cause invoke-constructor
    drive constructor, |SAXNotRecognizedException|
