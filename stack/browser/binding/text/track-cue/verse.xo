
force get-track
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |track|
    throw build

force get-id
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |id|
    throw build

force set-id
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |id|
    mount factor, share value

