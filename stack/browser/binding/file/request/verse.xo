
force get-locked-file
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |lockedFile|
    throw build

force get-onprogress
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |onprogress|
    throw build

force set-onprogress
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |onprogress|
    mount factor, share value
