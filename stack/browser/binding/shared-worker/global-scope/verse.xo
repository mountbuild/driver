
force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force get-application-cache
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |applicationCache|
    throw build

force close
  start build

  cause call-method
    mount object, share build
    mount method, |close|
