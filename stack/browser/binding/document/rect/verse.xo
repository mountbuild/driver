
force build-dom-rect
  start x
  start y
  start width
  start height

  cause invoke-constructor
    mount constructor, |DOMRect|
    mount factor, share x
    mount factor, share y
    mount factor, share width
    mount factor, share height
    throw build

force get-x
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |x|
    throw build

force set-x
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |x|
    mount factor, share value

