
force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force set-name
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |name|
    mount factor, share value

force get-label
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |label|
    throw build

force set-label
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |label|
    mount factor, share value
