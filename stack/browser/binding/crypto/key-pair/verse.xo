
force get-private-key
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |privateKey|
    throw build

force set-private-key
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |privateKey|
    mount factor, share value

force get-public-key
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |publicKey|
    throw build

force set-public-key
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |publicKey|
    mount factor, share value
