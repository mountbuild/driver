
force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force set-name
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |name|
    mount factor, share value

force get-salt-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |saltLength|
    throw build

force set-salt-length
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |saltLength|
    mount factor, share value
