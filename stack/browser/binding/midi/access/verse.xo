
force get-inputs
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |inputs|
    throw build

force get-outputs
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |outputs|
    throw build

force get-sysex-enabled
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |sysexEnabled|
    throw build
