
force get-frame
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |frame|
    throw build

force set-frame
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |frame|
    mount factor, share value

force get-input-source
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |inputSource|
    throw build

force set-input-source
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |inputSource|
    mount factor, share value
