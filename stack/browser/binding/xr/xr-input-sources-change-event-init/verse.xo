
force get-added
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |added|
    throw build

force get-removed
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |removed|
    throw build

force get-session
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |session|
    throw build
