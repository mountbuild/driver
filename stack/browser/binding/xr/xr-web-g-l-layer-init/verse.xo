
force get-alpha
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |alpha|
    throw build

force set-alpha
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |alpha|
    mount factor, share value

