
force get-max
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |max|
    throw build

force get-min
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |min|
    throw build

force get-value
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |value|
    throw build
