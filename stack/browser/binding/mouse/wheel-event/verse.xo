
force build-wheel-event
  cause invoke-constructor
    mount constructor, |WheelEvent|
    throw build

force get-delta-x
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |deltaX|
    throw build

force get-delta-y
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |deltaY|
    throw build

force get-delta-z
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |deltaZ|
    throw build

force get-delta-mode
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |deltaMode|
    throw build
