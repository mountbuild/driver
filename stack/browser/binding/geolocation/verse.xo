
force getcurrentposition-secure-context
  start build
  start success
  start error
  start options

  cause call-method
    mount object, share build
    mount method, |getCurrentPosition Secure context|
    mount factor, share success
    mount factor, share error
    mount factor, share options

force watchposition-secure-context
  start build
  start success
  start error
  start options

  cause call-method
    mount object, share build
    mount method, |watchPosition Secure context|
    mount factor, share success
    mount factor, share error
    mount factor, share options
    throw build

force clearwatch-secure-context
  start build
  start id

  cause call-method
    mount object, share build
    mount method, |clearWatch Secure context|
    mount factor, share id
