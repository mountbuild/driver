
force get-activated
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |activated|
    throw build

force get-has-reading
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |hasReading|
    throw build

force get-timestamp
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |timestamp|
    throw build

force start
  start build

  cause call-method
    mount object, share build
    mount method, |start|
    throw build

force stop
  start build

  cause call-method
    mount object, share build
    mount method, |stop|
