
force multi-draw-arrays-w-e-b-g-l
  start build
  start mode
  start firsts-list
  start firsts-offset
  start counts-list
  start counts-offset
  start draw-count

  cause call-method
    mount object, share build
    mount method, |multiDrawArraysWEBGL|
    mount factor, share mode
    mount factor, share firsts-list
    mount factor, share firsts-offset
    mount factor, share counts-list
    mount factor, share counts-offset
    mount factor, share draw-count
    throw build

