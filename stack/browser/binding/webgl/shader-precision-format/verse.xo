
force get-range-min
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |rangeMin|
    throw build

force get-range-max
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |rangeMax|
    throw build

force get-precision
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |precision|
    throw build
