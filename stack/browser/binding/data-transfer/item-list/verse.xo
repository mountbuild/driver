
force get-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |length|
    throw build

force add
  start build
  start data
  start type
  start file

  cause call-method
    mount object, share build
    mount method, |add|
    mount factor, share data
    mount factor, share type
    mount factor, share file
    throw build

force remove
  start build
  start index

  cause call-method
    mount object, share build
    mount method, |remove|
    mount factor, share index
    throw build

force clear
  start build

  cause call-method
    mount object, share build
    mount method, |clear|
    throw build

force data-transfer-item
  start build
  start index

  cause call-method
    mount object, share build
    mount method, |DataTransferItem|
    mount factor, share index
    throw build
