
force get-type
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |type|
    throw build

force set-type
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |type|
    mount factor, share value

force get-compact
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |compact|
    throw build

force set-compact
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |compact|
    mount factor, share value
