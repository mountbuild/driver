
force get-href
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |href|
    throw build

force set-href
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |href|
    mount factor, share value

force get-target
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |target|
    throw build

force set-target
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |target|
    mount factor, share value
