
force get-disabled
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |disabled|
    throw build

force set-disabled
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |disabled|
    mount factor, share value

force get-label
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |label|
    throw build

force set-label
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |label|
    mount factor, share value
