
force get-default-selected
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |defaultSelected|
    throw build

force set-default-selected
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |defaultSelected|
    mount factor, share value

