
force get-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |length|
    throw build

force get-selected-index
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |selectedIndex|
    throw build

force get-track-by-id
  start build
  start id

  cause call-method
    mount object, share build
    mount method, |getTrackById|
    mount factor, share id
    throw build
