
force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force close
  start build

  cause call-method
    mount object, share build
    mount method, |close|

force post-message
  start build
  start a-message
  start transfer-list
  start transfer-list

  cause call-method
    mount object, share build
    mount method, |postMessage|
    mount factor, share a-message
    mount factor, share transfer-list
    mount factor, share transfer-list
