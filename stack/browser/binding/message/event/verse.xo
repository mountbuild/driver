    throw build

force get-last-event-id
  start build

  cause get-aspect
    drive object, share build
    drive aspect, |lastEventId|
    throw build

force get-source
  start build

  cause get-aspect
    drive object, share build
    drive aspect, |source|
    throw build

force get-ports
  start build

  cause get-aspect
    drive object, share build
    drive aspect, |ports|
    throw build
