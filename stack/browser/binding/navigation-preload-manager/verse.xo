
force enable
  start build

  cause call-method
    mount object, share build
    mount method, |enable|

force disable
  start build

  cause call-method
    mount object, share build
    mount method, |disable|

force set-header-value
  start build

  cause call-method
    mount object, share build
    mount method, |setHeaderValue|

force get-state
  start build

  cause call-method
    mount object, share build
    mount method, |getState|
