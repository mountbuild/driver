
force get-description
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |description|
    throw build

force get-duration
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |duration|
    throw build

force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force to-j-s-o-n
  start build

  cause call-method
    mount object, share build
    mount method, |toJSON|
    throw build
