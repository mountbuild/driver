
force get-required-features
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |requiredFeatures|
    throw build

force get-required-extensions
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |requiredExtensions|
    throw build

force get-system-language
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |systemLanguage|
    throw build
