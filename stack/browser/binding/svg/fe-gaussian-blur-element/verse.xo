
force get-in1
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |in1|
    throw build

force get-std-deviation-x
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |stdDeviationX|
    throw build

force get-std-deviation-y
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |stdDeviationY|
    throw build

force get-edge-mode
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |edgeMode|
    throw build

force set-std-deviation
  start build

  cause call-method
    mount object, share build
    mount method, |setStdDeviation|
    throw build
