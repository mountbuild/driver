
force get-path-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |pathLength|
    throw build

force is-point-in-fill
  start build
  start point

  cause call-method
    mount object, share build
    mount method, |isPointInFill|
    mount factor, share point
    throw build

force is-point-in-stroke
  start build
  start point

  cause call-method
    mount object, share build
    mount method, |isPointInStroke|
    mount factor, share point
    throw build

force get-total-length
  start build

  cause call-method
    mount object, share build
    mount method, |getTotalLength|
    throw build

force get-point-at-length
  start build
  start distance

  cause call-method
    mount object, share build
    mount method, |getPointAtLength|
    mount factor, share distance
    throw build
