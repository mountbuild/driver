
force get-anim-val
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |animVal|
    throw build

force get-base-val
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |baseVal|
    throw build

force set-base-val
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |baseVal|
    mount factor, share value
