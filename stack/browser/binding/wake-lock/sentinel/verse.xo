
force get-released
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |released|
    throw build

force get-type
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |type|
    throw build

force release
  start build

  cause call-method
    mount object, share build
    mount method, |release|
    throw build
