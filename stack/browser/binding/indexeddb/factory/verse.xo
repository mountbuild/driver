
force open
  start build
  start name
  start version

  cause call-method
    mount object, share build
    mount method, |open|
    mount factor, share name
    mount factor, share version
    throw build

force delete-database
  start build
  start name
  start options

  cause call-method
    mount object, share build
    mount method, |deleteDatabase|
    mount factor, share name
    mount factor, share options
    throw build

force cmp
  start build
  start first
  start second

  cause call-method
    mount object, share build
    mount method, |cmp|
    mount factor, share first
    mount factor, share second
    throw build

force databases
  start build

  cause call-method
    mount object, share build
    mount method, |databases|
    throw build
