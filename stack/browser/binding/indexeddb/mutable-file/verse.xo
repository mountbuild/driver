
force get-name
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |name|
    throw build

force get-type
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |type|
    throw build

force open
  start build
  start range
  start direction

  cause call-method
    mount object, share build
    mount method, |open|
    mount factor, share range
    mount factor, share direction
    throw build

force get-file
  start build
  start range
  start direction

  cause call-method
    mount object, share build
    mount method, |getFile|
    mount factor, share range
    mount factor, share direction
    throw build
