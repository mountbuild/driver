
force get-detail
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |detail|
    throw build

force get-view
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |view|
    throw build

force inittimeevent-domstring-typearg-abstractview-viewarg-long-detailarg
  start build
  start id

  cause call-method
    mount object, share build
    mount method, |initTimeEvent(DOMString typeArg, AbstractView viewArg, long detailArg)|
    mount factor, share id
    throw build
