
force get-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |length|
    throw build

force set-length
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |length|
    mount factor, share value

force item
  start build

  cause call-method
    mount object, share build
    mount method, |item|

force named-item
  start build

  cause call-method
    mount object, share build
    mount method, |namedItem|
