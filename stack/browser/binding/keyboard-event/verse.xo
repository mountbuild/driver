    mount object, share build
    mount aspect, |isComposing|
    throw build

force get-key
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |key|
    throw build

force get-locale
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |locale|
    throw build

force get-location
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |location|
    throw build

force get-meta-key
  start build

  cause get-aspect
    mount object, share build
