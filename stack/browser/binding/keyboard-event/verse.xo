
force build-keyboard-event
  cause invoke-constructor
    mount constructor, |KeyboardEvent|
    throw build

force get-alt-key
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |altKey|
    throw build

force get-code
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |code|
    throw build

