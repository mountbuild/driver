
force get-selector-text
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |selectorText|
    throw build

force set-selector-text
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |selectorText|
    mount factor, share value

force get-style
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |style|
    throw build
