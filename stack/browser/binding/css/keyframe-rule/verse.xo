
force get-key-text
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |keyText|
    throw build

force set-key-text
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |keyText|
    mount factor, share value

force get-style
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |style|
    throw build
