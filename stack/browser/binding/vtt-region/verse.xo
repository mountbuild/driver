
force build-vtt-region
  start start-time
  start end-time
  start text

  cause invoke-constructor
    mount constructor, |VTTRegion|
    mount factor, share start-time
    mount factor, share end-time
    mount factor, share text
    throw build

force get-id
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |id|
    throw build

force set-id
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |id|
    mount factor, share value

