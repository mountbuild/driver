
force get-audio
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |audio|
    throw build

force set-audio
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |audio|
    mount factor, share value

force get-video
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |video|
    throw build

force set-video
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |video|
    mount factor, share value
