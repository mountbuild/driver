  start build

  cause get-aspect
    mount object, share build
    mount aspect, |previousRect|
    throw build

force set-previous-rect
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |previousRect|
    mount factor, share value

force get-current-rect
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |currentRect|
    throw build

force set-current-rect
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |currentRect|
    mount factor, share value
