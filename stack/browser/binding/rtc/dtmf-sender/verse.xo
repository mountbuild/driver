
force get-tone-buffer
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |toneBuffer|
    throw build

force insert-d-t-m-f
  start build
  start tones
  start duration
  start inter-tone-gap

  cause call-method
    mount object, share build
    mount method, |insertDTMF|
    mount factor, share tones
    mount factor, share duration
    mount factor, share inter-tone-gap
    throw build
