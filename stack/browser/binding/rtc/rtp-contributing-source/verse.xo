
force get-audio-level
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |audioLevel|
    throw build

force set-audio-level
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |audioLevel|
    mount factor, share value

