
force get-quota-secure-context
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |quota Secure context|
    throw build

force set-quota-secure-context
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |quota Secure context|
    mount factor, share value

