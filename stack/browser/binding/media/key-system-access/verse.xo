
force get-key-system
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |keySystem|
    throw build

force create-media-keys
  start build

  cause call-method
    mount object, share build
    mount method, |createMediaKeys|

force get-configuration
  start build

  cause call-method
    mount object, share build
    mount method, |getConfiguration|
