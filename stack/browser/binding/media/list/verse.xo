
force get-media-text
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |mediaText|
    throw build

force set-media-text
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |mediaText|
    mount factor, share value

force get-length
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |length|
    throw build

force append-medium
  start build

  cause call-method
    mount object, share build
    mount method, |appendMedium|

force delete-medium
  start build

  cause call-method
    mount object, share build
    mount method, |deleteMedium|

force item
  start build

  cause call-method
    mount object, share build
    mount method, |item|
