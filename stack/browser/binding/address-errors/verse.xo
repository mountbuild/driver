
force get-address-line
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |addressLine|
    throw build

force set-address-line
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |addressLine|
    mount factor, share value

