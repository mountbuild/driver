
force get-channel-count
  start build

  cause get-aspect
    mount object, share build
    mount aspect, |channelCount|
    throw build

force set-channel-count
  start build
  start value

  cause set-aspect
    mount object, share build
    mount aspect, |channelCount|
    mount factor, share value

